function user ( username, cousre , isLogged){
    this.username = username;
    this.cousre = cousre;
    this.isLogged = isLogged;
    this.getdata = function(){
        console.log(`welcome : ${username}`)
    }
    return this
}

const userone = new  user("vivek","js",ture);
